{
    "compilerOptions": {
      "jsx": "react-jsx",
      "target": "ES2015",
      "module": "commonjs",
      "strict": false,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true

    },
    "include": ["**/*.ts", "**/*.tsx"],
    "exclude": ["node_modules"],
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Recommended"
  }